{
    "name": "website",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently -k \"nodemon ./bin/www\" \"npm run watch\"",
        "watch": "webpack --watch --config webpack.config.js --mode development",
        "prod": "cross-env NODE_ENV=production node ./bin/www",
        "build": "webpack --config webpack.config.js --mode production",
        "demo-content": "node ./scripts/demo-content.js",
        "prebuild": "node ./scripts/prebuild.js"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cookie-session": "^2.0.0",
        "debug": "~2.6.9",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "file-type": "^18.7.0",
        "http-errors": "~1.6.3",
        "mongodb": "^6.2.0",
        "mongoose": "^8.0.0",
        "morgan": "~1.9.1",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.6.0",
        "sass": "^1.69.7",
        "sharp": "^0.32.6"
    },
    "devDependencies": {
        "browser-sync-webpack-plugin": "^2.3.0",
        "concurrently": "^8.2.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.9.0",
        "mini-css-extract-plugin": "^2.7.7",
        "nodemon": "^3.0.1",
        "sass-loader": "^13.3.3",
        "style-loader": "^3.3.4",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    }
